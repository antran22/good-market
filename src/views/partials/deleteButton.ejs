<% if (iconButton) { %>
    <a id="delete-<%= _id %>" class="btn btn-outline-danger float-end">
        <i class="bi bi-trash"></i>
    </a>
<% } else { %>
    <a id="delete-<%= _id %>" class="btn btn-danger">Delete</a>
<% } %>
<script>
  $(document).ready(() => {
    $("#delete-<%= _id %>").click(() => {
      if (confirm("Are you sure you want to delete?")) {
        window.location.href = "<%= target %>";
      }
    })
  });
</script>

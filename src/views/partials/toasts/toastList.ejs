<div id="toast-list" class="toast-container position-fixed top-0 end-0 p-3">
    <% for(let i = 0; i < errors.length; i++) { %>
        <%- await include("toast", {message: errors[i], toastClass: "bg-danger"}); %>
    <% } %>
    <% for(let i = 0; i < infos.length; i++) { %>
        <%- await include("toast", {message: infos[i], toastClass: "bg-primary"}); %>
    <% } %>
    <% for(let i = 0; i < successes.length; i++) { %>
        <%- await include("./toast", {message: successes[i], toastClass: "bg-success"}); %>
    <% } %>
</div>
